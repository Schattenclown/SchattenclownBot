<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ApplicationIcon>schattenclown.ico</ApplicationIcon>
    <Authors>Schattenclown, Lala Sabathil</Authors>
    <PackageProjectUrl>https://github.com/Schattenclown/SchattenclownBot</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Schattenclown/SchattenclownBot</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIcon>schattenclown-6.png</PackageIcon>
    <RepositoryType>git</RepositoryType>
    <AnalysisLevel>latest-all</AnalysisLevel>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>7</WarningLevel>
    <NoWarn>1701;1702;DV2001</NoWarn>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <WarningLevel>7</WarningLevel>
    <NoWarn>1701;1702;DV2001</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="FakesAssemblies\**" />
    <EmbeddedResource Remove="FakesAssemblies\**" />
    <None Remove="FakesAssemblies\**" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
    <None Include="..\icons\schattenclown-6.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DisCatSharp" Version="10.3.0" />
    <PackageReference Include="DisCatSharp.ApplicationCommands" Version="10.3.0" />
    <PackageReference Include="DisCatSharp.CommandsNext" Version="10.3.0" />
    <PackageReference Include="DisCatSharp.Interactivity" Version="10.3.0" />
    <PackageReference Include="DisCatSharp.Lavalink" Version="10.3.0" />
    <PackageReference Include="DisCatSharp.VoiceNext" Version="10.3.0" />
    <PackageReference Include="DisCatSharp.VoiceNext.Natives" Version="10.3.0" />
    <PackageReference Include="FubarCoder.RestSharp.Portable.Core" Version="4.0.8" />
    <PackageReference Include="MetaBrainz.MusicBrainz" Version="5.0.0" />
    <PackageReference Include="MySql.Data" Version="8.0.31" />
    <PackageReference Include="RestSharp" Version="108.0.3-alpha.0.5" />
    <PackageReference Include="SpotifyAPI.Web" Version="6.3.0" />
    <PackageReference Include="System.Drawing.Common" Version="7.0.0" />
    <PackageReference Include="TagLibSharp" Version="2.3.0" />
    <PackageReference Include="YoutubeDLSharp" Version="0.4.2" />
    <PackageReference Include="YoutubeExplode" Version="6.2.4" />
  </ItemGroup>

  <ItemGroup>
    <AdditionalDesignTimeBuildInput Remove="FakesAssemblies\**" />
  </ItemGroup>

</Project>
