[*.cs]

# DV0001: Ungültige Abhängigkeit
dotnet_diagnostic.dv0001.severity = none

# DV2001: Kein Abhängigkeitsdiagramm verknüpft
dotnet_diagnostic.dv2001.severity = warning

# CA1416: Plattformkompatibilität überprüfen
dotnet_diagnostic.ca1416.severity = silent
indent_style = space
indent_size = 3
tab_width = 3

# Microsoft .NET properties
csharp_preferred_modifier_order = public, private, protected, internal, new, static, abstract, virtual, sealed, readonly, override, extern, unsafe, volatile, async, required:suggestion
csharp_style_var_elsewhere = false:suggestion
csharp_style_var_for_built_in_types = false:suggestion
csharp_style_var_when_type_is_apparent = false:suggestion
